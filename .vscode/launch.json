  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "npx tenderly login",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/dist/internal/cli/index.js",
      "args": ["login"],
      "outFiles": ["${workspaceFolder}/dist/**/*.js"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "npx tenderly networks",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/dist/internal/cli/index.js",
      "args": ["networks"],
      "outFiles": ["${workspaceFolder}/dist/**/*.js"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "npx tenderly vnet",
      "skipFiles": ["<node_internals>/**"],
      "cwd": "${workspaceFolder}/examples/contract-verification",
      "program": "${workspaceFolder}/dist/internal/cli/index.js",
      "args": ["vnet"],
      "outFiles": ["${workspaceFolder}/dist/**/*.js"]
    }
  ]
}
